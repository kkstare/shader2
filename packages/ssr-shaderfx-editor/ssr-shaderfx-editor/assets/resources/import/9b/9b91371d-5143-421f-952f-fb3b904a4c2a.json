[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ImageWhiteBalance",{"name":"ImageWhiteBalance","name_zh":"白平衡","version":"1.0","description":{"en":"Adjusts the temperature and tint of input value by the amount of inputs Temperature and Tint.","zh":"调整输入图像的白平衡。"},"tags":["Core","Image","Color"],"input":[{"type":"vec4","name":"Color","info":{"en":"Input value","zh":"输入值"}},{"type":"float","name":"Temperature","info":{"en":"Temperature value","zh":"调整该值会将输出向黄，蓝色变化"}},{"type":"float","name":"Tint","info":{"en":"Tint value","zh":"调整该值会将输出向粉红，绿色变化"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"}],"uniforms":{},"frags":{},"main":["vec4 main (vec4 Color, float Temperature, float Tint) {","    float t1 = Temperature * 10.0 / 6.0;","    float t2 = Tint * 10.0 / 6.0;","    float x = 0.31271 - t1 * (t1 < 0.0 ? 0.1 : 0.05);","    float standardIlluminantY = 2.87 * x - 3.0 * x * x - 0.27509507;","    float y = standardIlluminantY + t2 * 0.05;","    vec3 w1 = vec3(0.949237, 1.03542, 1.08728);","    float Y = 1.0;","    float X = Y * x / y;","    float Z = Y * (1.0 - x - y) / y;","    float L = 0.7328 * X + 0.4296 * Y - 0.1624 * Z;","    float M = -0.7036 * X + 1.6975 * Y + 0.0061 * Z;","    float S = 0.0030 * X + 0.0136 * Y + 0.9834 * Z;","    vec3 w2 = vec3(L, M, S);","    vec3 balance = vec3(w1.x / w2.x, w1.y / w2.y, w1.z / w2.z);","    mat3 LIN_2_LMS_MAT = mat3(","        3.90405e-1, 5.49941e-1, 8.92632e-3,","        7.08416e-2, 9.63172e-1, 1.35775e-3,","        2.31082e-2, 1.28021e-1, 9.36245e-1","    );","    mat3 LMS_2_LIN_MAT = mat3(","        2.85847e+0, -1.62879e+0, -2.48910e-2,","        -2.10182e-1,  1.15820e+0,  3.24281e-4,","        -4.18120e-2, -1.18169e-1,  1.06867e+0","    );","    vec3 lms = LIN_2_LMS_MAT * Color.rgb;","    lms *= balance;","    return vec4(LMS_2_LIN_MAT * lms, Color.a);","}"]}]],0,0,[],[],[]]