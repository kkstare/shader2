[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ImageContrast",{"name":"ImageContrast","name_zh":"对比度调整","version":"1.0","description":{"en":"Adjusts the contrast of input value by the amount of input Contrast.","zh":"调整输入图像的对比度。"},"tags":["Core","Image","Color"],"input":[{"type":"vec4","name":"Color","info":{"en":"Input value","zh":"输入值"}},{"type":"float","name":"Contrast","info":{"en":"Contrast value","zh":"对比度"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"}],"uniforms":{},"frags":{},"main":["vec4 main (vec4 Color, float Contrast) {","    float midpoint = pow(0.5, 2.2);","    vec3 result = (Color.rgb - midpoint) * Contrast + midpoint;","    return vec4(result, Color.a);","}"]}]],0,0,[],[],[]]