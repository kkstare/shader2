[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"UVRotate",{"name":"UVRotate","name_zh":"旋转","version":"1.0","description":{"en":"Rotates value of input UV around a reference point by a given radians.","zh":"将输入的纹理坐标绕给定的一个点旋转一定的弧度。"},"tags":["Core","UV "],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"vec2","name":"Center","info":{"en":"Center reference point","zh":"中心点"}},{"type":"float","name":"Radians","info":{"en":"Amount of rotation to appl in Radians","zh":"旋转弧度"}}],"output":[{"type":"vec2","name":"UV"},{"type":"float","name":"x","property":"x"},{"type":"float","name":"y","property":"y"}],"uniforms":{},"frags":{},"main":["vec2 main (vec2 UV, vec2 Center, float Radians) {","    UV -= Center;","    float s = sin(Radians);","    float c = cos(Radians);","    mat2 rMatrix = mat2(c, -s, s, c);","    rMatrix *= 0.5;","    rMatrix += 0.5;","    rMatrix = rMatrix * 2.0 - 1.0;","    UV.xy *= rMatrix;","    UV += Center;","    return UV;","}"]}]],0,0,[],[],[]]