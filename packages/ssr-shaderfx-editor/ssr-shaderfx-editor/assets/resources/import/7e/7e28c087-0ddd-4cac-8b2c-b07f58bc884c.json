[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ShapeEllipse",{"name":"ShapeEllipse","name_zh":"椭圆形","version":"1.0","fx":true,"description":{"en":"Generates an ellipse shape based on input UV.","zh":"使用输入的纹理坐标，以及指定参数生成一个椭圆形。"},"tags":["Core","Procedural","Shape"],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"float","name":"Width","info":{"en":"Ellipse width","zh":"椭圆宽度"}},{"type":"float","name":"Height","info":{"en":"Ellipse height","zh":"椭圆高度"}}],"output":[{"type":"float","name":"Output"}],"uniforms":{},"frags":{},"main":["float main (vec2 UV, float Width, float Height) {","    float d = length((UV * 2.0 - 1.0) / vec2(Width, Height));","    #if defined GL_OES_standard_derivatives","        return clamp((1.0 - d) / fwidth(d), 0.0, 1.0);","    #else","        return clamp((1.0 - d) / d, 0.0, 1.0);","    #endif","}"]}]],0,0,[],[],[]]