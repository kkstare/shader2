[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"Stone",{"name":"Stone","category":"Stone","version":"1.0","author":"SuperSuRaccoon","info":"Stone.","description":"Stone.","refs":[],"tags":[],"input":[{"type":"vec2","name":"uv","info":"uv"},{"type":"sampler2D","name":"tex","info":"vec3 to split"}],"output":[{"type":"vec4","name":"rgba","info":"rgba"}],"uniforms":{"intensity":{"type":"float","min":0,"max":5,"default":1},"deepness":{"type":"float","min":0,"max":5,"default":1}},"frags":{"rainbow":[" vec4 rainbow(float t) {","     t = mod(t, 1.0);","     float tx = t * 6.0;","     float r = clamp(tx - 2.0, 0.0, 1.0) + clamp(2.0 - tx, 0.0, 1.0);","     return vec4(1.0, 1.0, 1.0, r);"," }"],"plasma":[" vec4 plasma(vec2 uv, sampler2D tex) {","     vec2 tuv = uv;","     uv *= 15.0;","     float a = 46.1;","     float n = sin(a + 2.0 * uv.x) + sin(a - 2.0 * uv.x) + sin(a + 2.0 * uv.y) + sin(a + 5.0 * uv.y);","     n = mod(((5.0 + n) / 5.0), 1.0);","     n += texture2D(tex, tuv).r * 11.2 + texture2D(tex, tuv).g * 8.4 + texture2D(tex, tuv).b * 4.2;","     return rainbow(n * deepness);"," }"]},"main":["vec4 main(vec2 uv, sampler2D tex) {","    vec4 texColor = texture2D(tex, uv);","    float lum = (texColor.r + texColor.b + texColor.g) / 3.0;","    float rate = 0.2;","    float r = lum * 0.8;","    r = r + (1.0 * (rate / 2.0));","    if (r > 0.6) r = 0.6;","    if (r < 0.2) r = 0.2;","    vec4 stone = plasma(uv, tex);","    stone += plasma(vec2(uv.y, uv.x), tex);","    texColor.rgb = mix(texColor.rgb, vec3(r - (1.0 - stone.a) / 8.0), intensity);","    return texColor;","}"]}]],0,0,[],[],[]]