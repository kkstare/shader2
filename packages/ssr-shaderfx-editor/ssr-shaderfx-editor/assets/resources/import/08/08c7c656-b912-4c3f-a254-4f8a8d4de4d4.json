[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"BlendExclusion",{"name":"BlendExclusion","name_zh":"排除模式","version":"1.0","description":{"en":"Blends the input value Blend onto input Base using the Exclusion blend mode.","zh":"使用 [排除模式] 混合两个输入的图层。"},"tags":["Color","Blend","Image"],"input":[{"type":"vec4","name":"Base","info":{"en":"Base layer value","zh":"基础图层值"}},{"type":"vec4","name":"Blend","info":{"en":"Blend layer value","zh":"混合图层值"}},{"type":"float","name":"Opacity","info":{"en":"Strength of blend","zh":"混合强度值"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"},{"type":"float","name":"r","property":"r"},{"type":"float","name":"g","property":"g"},{"type":"float","name":"b","property":"b"},{"type":"float","name":"a","property":"a"}],"uniforms":{},"frags":{},"main":["vec4 main(vec4 Base, vec4 Blend, float Opacity) {","    return mix(Base, Blend + Base - (2.0 * Blend * Base), Opacity);","}"]}]],0,0,[],[],[]]