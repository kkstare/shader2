[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"BarrelBlur",{"name":"BarrelBlur","name_zh":"桶状模糊","author":"supersuraccoon","custom":false,"official":false,"fx":true,"version":"1.0","objectId":"60a275406d8bee18f60349e7","category":{"en":"blur","zh":"模糊特效"},"description":{"en":"Barrel Blur","zh":"桶状模糊"},"tags":["FX","Blur"],"input":[{"type":"sampler2D","name":"tex"},{"type":"vec2","name":"uv"}],"output":[{"type":"vec4","name":"Output"}],"uniforms":{"uIntensity":{"type":"float","default":1}},"frags":{"barrelDistortion":["vec2 barrelDistortion(vec2 coord, float amt) {","    vec2 cc = coord - 0.5;","    float dist = dot(cc, cc);","    return coord + cc * dist * amt;  ","}"],"sat":["","float sat(float t) {","    return clamp(t, 0.0, 1.0);","}"],"linterp":["","float linterp(float t) {","    return sat(1.0 - abs(2.0 * t - 1.0));","}"],"remap":["","float remap(float t, float a, float b) {","    return sat((t - a) / (b - a));","}"],"spectrumOffset":["","vec3 spectrumOffset(float t) {","    vec3 ret;","    float lo = step(t, 0.5);","    float hi = 1.0 - lo;","    float w = linterp(remap(t, 1.0 / 6.0, 5.0 / 6.0));","    ret = vec3(lo, 1.0, hi) * vec3(1.0 - w, w, 1.0 - w);","    return pow(ret, vec3(1.0 / 2.2));","}"]},"main":["","vec4 main(sampler2D tex, vec2 uv) ","{","    vec3 sumcol = vec3(0.0);","    vec3 sumw = vec3(0.0);","    for (int i = 0; i < 10; ++i) {","        float t = float(i) * 0.1;","        vec3 w = spectrumOffset(t);","        sumw += w;","        vec2 distortUV = barrelDistortion(uv, uIntensity * t);","        vec4 color = texture2D(tex, distortUV);","        sumcol += w * color.rgb;","    }","    return vec4(sumcol / sumw, texture2D(tex, uv).a);   ","}","    "]}]],0,0,[],[],[]]