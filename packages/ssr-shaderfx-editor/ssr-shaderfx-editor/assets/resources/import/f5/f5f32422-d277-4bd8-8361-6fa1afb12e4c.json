[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ColorHSV2RGB",{"name":"ColorHSV2RGB","name_zh":"HSV转RGB","version":"1.0","description":{"en":"Converts the input value from HSV colorspace to RGB.","zh":"将输入值从HSV色彩空间转换到RGB色彩空间。"},"tags":["Core","Color","Convert"],"input":[{"type":"vec4","name":"Color","info":{"en":"Input value","zh":"输入值"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"},{"type":"float","name":"r","property":"r"},{"type":"float","name":"g","property":"g"},{"type":"float","name":"b","property":"b"},{"type":"float","name":"a","property":"a"}],"uniforms":{},"frags":{},"main":["vec4 main (vec4 Color) {","    vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);","    vec3 P = abs(fract(Color.xxx + K.xyz) * 6.0 - K.www);","    return vec4(Color.z * mix(K.xxx, clamp(P - K.xxx, 0.0, 1.0), Color.y), Color.a);","}"]}]],0,0,[],[],[]]