[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"FireGenerator",{"name":"FireGenerator","category":"FireGenerator","version":"1.0","author":"SuperSuRaccoon","info":"FireGenerator.","description":"FireGenerator.","refs":[],"tags":["Core"],"input":[{"type":"vec2","name":"uv","info":"uv"}],"output":[{"type":"vec4","name":"rgba","info":"rgba"}],"uniforms":{"offset_x":{"type":"float","min":-1,"max":1,"default":0},"offset_y":{"type":"float","min":-1,"max":1,"default":0},"intensity":{"type":"float","min":0,"max":1,"default":0.5},"smooth":{"type":"float","min":0,"max":5,"default":1},"speed":{"type":"float","min":0,"max":5,"default":1},"alpha":{"type":"float","min":0,"max":1,"default":0.5}},"frags":{"rand2d":["float rand2d(vec2 uv) { ","    return fract(sin(dot(uv, vec2(13.454, 7.405))) * 12.3043); ","}"],"voronoi2d":["float voronoi2d(vec2 uv, float intensity) { ","    vec2 fl = floor(uv);","    vec2 fr = fract(uv);","    float res = 1.0; ","    for (int j = -1; j <= 1; j++) {","        for (int i = -1; i <= 1; i++) {","            vec2 p = vec2(i, j);","            float h = rand2d(fl + p);","            vec2 vp = p - fr + h; ","            float d = dot(vp, vp); ","            res += 1.0 / pow(d, 2.0); ","        }","    } ","    return pow(1.0 / res, intensity); "," }"]},"main":["vec4 main(vec2 uv) {","    uv += vec2(offset_x, offset_y);","    float t = CC_Time[0] * -50. * speed;","    float up0 = voronoi2d(uv * vec2(6.0,4.0) + vec2(0., -t), intensity);","    float up1 = 0.500 + voronoi2d(uv * vec2(6.0, 4.0) + vec2(42, -t) + 30.0, intensity);","    float finalMask = up0 * up1 + (uv.y);","    finalMask += (uv.y) * 0.5;","    finalMask *= 0.7 - abs(uv.x - 0.5);","    float s = smoothstep(smooth, 0.9, finalMask);","    vec4 result = vec4(s);","    result.a = clamp(result.a + alpha, 0.0, 1.0);","    return result;","}"]}]],0,0,[],[],[]]