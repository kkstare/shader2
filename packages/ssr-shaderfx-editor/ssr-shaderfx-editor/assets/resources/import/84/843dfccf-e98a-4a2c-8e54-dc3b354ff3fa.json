[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"UVSpherize",{"name":"UVSpherize","name_zh":"球形化","version":"1.0","description":{"en":"Applies a spherical warping effect (fisheye camera lens) to the input UV.","zh":"将输入的纹理坐标进行类似鱼眼特效的球形化处理。"},"tags":["Core","UV "],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"vec2","name":"Center","info":{"en":"Center reference point","zh":"中心点"}},{"type":"float","name":"Strength","info":{"en":"Strength of the effect","zh":"特效强度"}},{"type":"vec2","name":"Offset","info":{"en":"Individual channel offsets","zh":"偏移值"}}],"output":[{"type":"vec2","name":"UV"},{"type":"float","name":"x","property":"x"},{"type":"float","name":"y","property":"y"}],"uniforms":{},"frags":{},"main":["vec2 main (vec2 UV, vec2 Center, float Strength, vec2 Offset) {","    vec2 delta = UV - Center;","    float delta2 = dot(delta.xy, delta.xy);","    float delta4 = delta2 * delta2;","    vec2 delta_offset = vec2(delta4 * Strength);","    return UV + delta * delta_offset + Offset;","}"]}]],0,0,[],[],[]]