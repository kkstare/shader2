[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"WaveNoiseSine",{"name":"WaveNoiseSine","name_zh":"正弦噪声波","version":"1.0","description":{"en":"Returns the sine of the input value.","zh":"使用输入值，生成正弦噪声波。"},"tags":["Core","Noise","Random","Generator","Wave"],"input":[{"type":"float","name":"Input","info":{"en":"Input value","zh":"输入值"}},{"type":"float","name":"Min","info":{"en":"Minimum value for noise intensity","zh":"噪声强度最小值"}},{"type":"float","name":"Max","info":{"en":"Maximum value for noise intensity","zh":"噪声强度最大值"}}],"output":[{"type":"float","name":"Output"}],"uniforms":{},"frags":{},"main":["float main (float Input, float Min, float Max) {","    float sinIn = sin(Input);","    float sinInOffset = sin(Input + 1.0);","    float randomno =  fract(sin((sinIn - sinInOffset) * (12.9898 + 78.233)) * 43758.5453);","    float noise = mix(Min, Max, randomno);","    return sinIn + noise;","}"]}]],0,0,[],[],[]]