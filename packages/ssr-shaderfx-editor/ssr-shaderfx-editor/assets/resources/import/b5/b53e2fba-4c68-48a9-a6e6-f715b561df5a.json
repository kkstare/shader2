[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ProceduralClover",{"name":"ProceduralClover","name_zh":"四叶草","version":"1.0","fx":true,"description":{"en":"Generates a rotating clover based on input UV.","zh":"使用输入的纹理坐标，以及指定颜色生成旋转的四叶草。"},"tags":["Core","Procedural","Animated","Time"],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"vec4","name":"Color","info":{"en":"Clover color","zh":"四叶草的颜色"}},{"type":"float","name":"Seed","info":{"en":"Time seed for animation","zh":"时间种子，用于动画"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"}],"uniforms":{},"frags":{},"main":["vec4 main (vec2 UV, vec4 Color, float Seed) {","    float a = atan(UV.x, UV.y);","    float r = length(UV);","    float s = 0.5 + 0.5 * sin(3.0 * a + Seed);","    float g = sin(1.57 + 3.0 * a + Seed);","    float d = 0.15 + 0.3 * sqrt(s) + 0.15 * g * g;","    float h = r / d;","    float f = 1.0 - smoothstep(0.95, 1.0, h);","    h *= 1.0 - 0.5 * (1.0 - h) * smoothstep(0.95 + 0.05 * h, 1.0, sin(3.0 * a + Seed));","    vec4 bcol = vec4(0.0);","    h = 0.1 + h;","    return mix(bcol, 1.2 * vec4(Color.r * h, Color.g * h, Color.b, Color.a), f);","}"]}]],0,0,[],[],[]]