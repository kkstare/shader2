[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"UVAtlas",{"name":"UVAtlas","name_zh":"图集坐标","version":"1.0","description":{"en":"Convert the input texture uv coord to atlas coord.","zh":"将输入的纹理坐标，转换到图集中的坐标。"},"tags":["Core","UV"],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"vec4","name":"UVRect","info":{"en":"Atlas uv rect info","zh":"图集矩形信息"}}],"output":[{"type":"vec2","name":"UV"},{"type":"float","name":"x","property":"x"},{"type":"float","name":"y","property":"y"}],"uniforms":{},"frags":{"f_atlas_uv_linear":["float f_atlas_uv_linear(float x1, float y1, float x2, float y2, float val) {","    return (y2 - x2) * (val - x1) / (y1 - x1) + x2;","}"],"f_atlas_uv_linear_to_01":["vec2 f_atlas_uv_linear_to_01(vec4 uvrect, vec2 uv) {","    return vec2(","        f_atlas_uv_linear(uvrect[0], uvrect[0] + uvrect[2], 0.0, 1.0, uv.x),","        f_atlas_uv_linear(uvrect[1], uvrect[1] + uvrect[3], 0.0, 1.0, uv.y)","    );","}"]},"main":["vec2 main(vec2 UV, vec4 UVRect) {","    return f_atlas_uv_linear_to_01(UVRect, UV);","}"]}]],0,0,[],[],[]]