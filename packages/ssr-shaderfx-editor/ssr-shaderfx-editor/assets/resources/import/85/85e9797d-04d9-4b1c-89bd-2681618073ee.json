[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ShapeRectangle",{"name":"ShapeRectangle","name_zh":"矩形","version":"1.0","fx":true,"description":{"en":"Generates a rectangle shape based on input UV.","zh":"使用输入的纹理坐标，以及指定参数生成一个矩形。"},"tags":["Core","Procedural","Shape"],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"float","name":"Width","info":{"en":"Rectangle width","zh":"矩形宽度"}},{"type":"float","name":"Height","info":{"en":"Rectangle height","zh":"矩形高度"}}],"output":[{"type":"float","name":"Output"}],"uniforms":{},"frags":{},"main":["float main (vec2 UV, float Width, float Height) {","    vec2 d = abs(UV * 2.0 - 1.0) - vec2(Width, Height);","    #if defined GL_OES_standard_derivatives","        d = 1.0 - d / fwidth(d);","    #else","        d = 1.0 - d;","    #endif","    return clamp(min(d.x, d.y), 0.0, 1.0);","}"]}]],0,0,[],[],[]]