[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ShapeRoundedRectangle",{"name":"ShapeRoundedRectangle","name_zh":"圆角矩形","version":"1.0","fx":true,"description":{"en":"Generates a rounded rectangle shape based on input UV.","zh":"使用输入的纹理坐标，以及指定参数生成一个圆角矩形。"},"tags":["Core","Procedural","Shape"],"input":[{"type":"vec2","name":"UV","info":{"en":"UV input value","zh":"UV 输入值"}},{"type":"float","name":"Width","info":{"en":"Rounded rectangle width","zh":"圆角矩形宽度"}},{"type":"float","name":"Height","info":{"en":"Rounded rectangle height","zh":"圆角矩形高度"}},{"type":"float","name":"Radius","info":{"en":"Corner radius","zh":"圆角半径"}}],"output":[{"type":"float","name":"Output"}],"uniforms":{},"frags":{},"main":["float main (vec2 UV, float Width, float Height, float Radius) {","    Radius = max(min(min(abs(Radius * 2.0), abs(Width)), abs(Height)), 1e-5);","    vec2 uv = abs(UV * 2.0 - 1.0) - vec2(Width, Height) + Radius;","    float d = length(max(vec2(0.0), uv)) / Radius;","    return clamp((1.0 - d) / d, 0.0, 1.0);","}"]}]],0,0,[],[],[]]