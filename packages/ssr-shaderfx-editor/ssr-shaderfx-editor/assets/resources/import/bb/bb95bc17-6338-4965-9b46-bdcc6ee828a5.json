[1,0,0,[["cc.JsonAsset",["_name","json"],1]],[[0,0,1,3]],[[0,"ColorHSV2Linear",{"name":"ColorHSV2Linear","name_zh":"HSV转线性RGB","version":"1.0","description":{"en":"Converts the input value from HSV colorspace to Linear.","zh":"将输入值从HSV色彩空间转换到线性RGB色彩空间。"},"tags":["Core","Color","Convert"],"input":[{"type":"vec4","name":"Color","info":{"en":"Input value","zh":"输入值"}}],"output":[{"type":"vec4","name":"Color"},{"type":"vec3","name":"rgb","property":"rgb"},{"type":"float","name":"r","property":"r"},{"type":"float","name":"g","property":"g"},{"type":"float","name":"b","property":"b"},{"type":"float","name":"a","property":"a"}],"uniforms":{},"frags":{},"main":["vec4 main (vec4 Color) {","    vec4 K = vec4(1.0, 2.0 / 3.0, 1.0 / 3.0, 3.0);","    vec3 P = abs(fract(Color.xxx + K.xyz) * 6.0 - K.www);","    vec3 RGB = Color.z * mix(K.xxx, clamp(P - K.xxx, 0.0, 1.0), Color.y);","    vec3 linearRGBLo = RGB / 12.92;","    vec3 linearRGBHi = pow(max(abs((RGB + 0.055) / 1.055), 1.192092896e-07), vec3(2.4, 2.4, 2.4));","    return vec4( ","        RGB.r <= 0.04045 ? linearRGBLo.r : linearRGBHi.r, ","        RGB.g <= 0.04045 ? linearRGBLo.g : linearRGBHi.g, ","        RGB.b <= 0.04045 ? linearRGBLo.b : linearRGBHi.b, ","        Color.a","    );","}"]}]],0,0,[],[],[]]